	<rdf:Description rdf:about="https://en.wikipedia.org/wiki/Raja_Rafe">
		<rdf:type rdf:resource="myrdfs:bestFootballPlayers"/>
		<myrdfs:bestFootballPlayer_id>1</myrdfs:bestFootballPlayer_id>
		<foaf:name>Raja Rafe</foaf:name>
		<foaf:mbox rdf:resource="mailto:Raja@football.org"/>
